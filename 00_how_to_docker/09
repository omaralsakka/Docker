docker network create omar-nw
docker run --name spawning-pool --network omar-nw --restart on-failure:5 -v /var/lib/docker/volumes/hatchery:/var/lib/mariadb -e MARIADB_ROOT_PASSWORD=Kerrigan -e MARIADB_DATABASE=zerglings -it -d mariadb
